    "@tanstack/router": "^0.0.1-beta.53",
    "@tanstack/router-devtools": "^1.121.21",
    "axios": "^1.10.0",
    "date-fns": "^4.1.0",
    "express": "^4.21.2",
    "framer-motion": "^12.18.1",
    "fuse.js": "^7.1.0",
    "gray-matter": "^4.0.3",
    "keen-slider": "^6.8.6",
    "next-themes": "^0.4.6",
